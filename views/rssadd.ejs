<div class="modal fade rssAddmodal" tabindex="-1" role="dialog" aria-labelledby="myrssaddmodal" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
    	<div class="modal-header">
          <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
          <h4 class="modal-title" id="myrssaddmodal">Modal title</h4>
        </div>
        <div class="modal-body">
	      <form class="form-horizontal" id="rssaddform">
	      	<div class="form-group">
			    <label for="inputrssname" class="col-sm-2 control-label">来源</label>
			    <div class="col-sm-10">
			      <input type="text" required class="form-control" id="inputrssname" placeholder="From" name="rssfrom" autocomplete="off">
			    </div>
			  </div>
	      	  <div class="form-group">
			    <label for="inputrssname" class="col-sm-2 control-label">名称</label>
			    <div class="col-sm-10">
			      <input type="text" required class="form-control" id="inputrssname" placeholder="Name" name="rssmainname" autocomplete="off">
			    </div>
			  </div>
			  <div class="form-group">
			    <label  class="col-sm-2 control-label">类别</label>
			    <div class="col-sm-10">
			      	<select class="form-control" name="rsstype">
					  <% for(var i=0;i<rsstypelist.length;i++) {%>
					  	<option value='<%=rsstypelist[i].typeid%>'> <%=rsstypelist[i].typename%> </option>
					  <% } %>
					</select>
			    </div>
			  </div>
			  <div class="form-group">
			    <label for="inputrssurl" class="col-sm-2 control-label">地址</label>
			    <div class="col-sm-8">
			      <input required type="text" name="rssurl" class="form-control" id="inputrssurl" placeholder="Url" autocomplete="off">
			    </div>
			    <div class="col-sm-2">
			    	<button type="button" class="btn btn-primary" onclick="javascript:window.open($('#inputrssurl').val());$('#btnaddrss').removeAttr('disabled').removeClass('btn-default').addClass('btn-primary')">预览</button>
			    </div>
			  </div>
			  <!-- <div class="collapse" id="extendrssinfo">
				  
			  </div> -->
	      </form>
	    </div>
	    <div class="modal-footer">
		    <div class="center-block" style="display:inline-block">
		      <button type="button" class="btn btn-default" id="btnaddrss" disabled="disabled">确认</button>
		      <button type="button" class="btn btn-default"  data-dismiss="modal">关闭</button>
		    </div>
	  	</div>
    </div>
  </div>
</div>